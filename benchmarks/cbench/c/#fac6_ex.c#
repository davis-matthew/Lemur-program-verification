qvoid assert(int cond) { if (!(cond)) { ERROR : { reach_error(); abort(); } } }
void assume(int cond) { if (!cond) { abort(); } }

int h = 0;

int
main()
{
  int M = 1000000000;
  int D = 9;
  int n, m, L, i;
  n = __VERIFIER_nondet_int();


  L = 0;
  for (m = n; m; m = m/10){
    L++;
  }
  L = L*n/D + 1;

  assume(n > 0);
  assume(L == 1);

  unsigned a0;

  unsigned b, c;
  int l;

  a0 = 1;
  l = 1;
  while (n) {
    assume(a0 <= M);
    c = 0;
    for (i = 0; i < l || c; i++) {
      b = n*a0 + c;
      c = b/M;
      a0 = b - c*M;
    }
    if (i > l)
      l = i;
    n--;
  }
  L = l;

  assert(a0 <= M);
  return 0;
}
